{
  "name": "@leesuhyung/cra-template-typescript",
  "version": "1.0.14",
  "author": "leesuhyung <realnavystyle@gmail.com>",
  "engines": {
    "node": ">=17"
  },
  "main": "template.json",
  "description": "A quick start Create React App template with React, TypeScript, Custom ESlint configurations",
  "license": "MIT",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "cra-template-typescript",
    "eslint",
    "typescript",
    "prettier",
    "leesuhyung"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leesuhyung/cra-template-typescript",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/leesuhyung/cra-template-typescript/issues"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "husky install && husky set .husky/prepare-commit-msg 'exec < /dev/tty && ./node_modules/.bin/cz --hook || true'",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "semantic-release": "semantic-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.49.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "pinst": "3.0.0",
    "semantic-release": "21.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
